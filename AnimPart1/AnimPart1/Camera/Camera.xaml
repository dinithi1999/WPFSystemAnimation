<UserControl x:Class="AnimPart1.Camera.Camera"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AnimPart1.Camera"
             mc:Ignorable="d" Height="230" Width="353">


    <Canvas x:Name="MainCanvas" Margin="-2,-102,-36,-9" RenderTransformOrigin="0.5,0.5" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" >
        <Canvas.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform AngleX="-0.766"/>
                <RotateTransform/>
                <TranslateTransform X="-0.474"/>
            </TransformGroup>
        </Canvas.RenderTransform>
    </Canvas>

</UserControl>
