<UserControl x:Class="AnimPart1.UC_OtherAnima.Pipes.UserControlBluePipes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AnimPart1.UC_OtherAnima.Pipes" 
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             MouseRightButtonDown="UserControl_MouseRightButtonDown" 

             mc:Ignorable="d">
    <UserControl.Resources>
        <Storyboard x:Key="pulse1Animation">
            <DoubleAnimation
                Storyboard.TargetName="translatePulse1"
                Storyboard.TargetProperty="Y"
                From="0"
                To="80" 
                RepeatBehavior="Forever"
                AutoReverse="False"
                Duration="0:0:1">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse2"
                Storyboard.TargetProperty="Y"
                From="0"
                To="80" 
                RepeatBehavior="Forever"
                AutoReverse="False"
                Duration="0:0:1">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse3"
                Storyboard.TargetProperty="Y"
                From="0"
                To="80" 
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:1">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse4"
                Storyboard.TargetProperty="Y"
                From="0"
                To="80" 
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:1">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse5"
                Storyboard.TargetProperty="Y"
                From="0"
                To="180" 
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:2">
            </DoubleAnimation>

            <DoubleAnimation
                Storyboard.TargetName="translatePulseX1"
                Storyboard.TargetProperty="X"
                From="0"
                To="80" 
                AutoReverse ="False"
                RepeatBehavior="Forever"
                Duration="0:0:1">
            </DoubleAnimation>

        </Storyboard>

        <ContextMenu x:Key="ContextMenu1">
            <MenuItem x:Name="menuItem1" Header="Close FPU Valve" InputGestureText="Shift+L" Click="Option1_Click" Style="{StaticResource CustomMenuItemStyle}"/>
            <MenuItem x:Name="menuItem2" Header="Open FPU Valve" InputGestureText="Shift+C" Click="Option2_Click" Style="{StaticResource CustomMenuItemStyle}"/>
        </ContextMenu>
        
    </UserControl.Resources>

    <Grid x:Name="MainGrid" Margin="0,234,-33,48">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <!--Source="pack://application:,,,/UC_OtherAnima/FPUAnimation/Images/ellowPulse.svg"-->
