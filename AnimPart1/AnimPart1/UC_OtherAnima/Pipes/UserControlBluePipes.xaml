                Duration="0:0:2">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse4"
                Storyboard.TargetProperty="Y"
                From="0"
                To="60" 
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:2">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse5"
                Storyboard.TargetProperty="Y"
                From="0"
                To="60" 
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:2">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse6"
                Storyboard.TargetProperty="Y"
                From="0"
                To="60"
                AutoReverse="False"
                RepeatBehavior="Forever"
                Duration="0:0:2">
            </DoubleAnimation>
          
            <DoubleAnimation
                Storyboard.TargetName="translatePulseX1"
                Storyboard.TargetProperty="X"
                From="0"
                To="30" 
                AutoReverse ="False"
                RepeatBehavior="Forever"
                Duration="0:0:2">
            </DoubleAnimation>
           
        </Storyboard>
