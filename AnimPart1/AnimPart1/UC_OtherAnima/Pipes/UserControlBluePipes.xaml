        <Storyboard x:Key="pulse1Animation">
            <DoubleAnimation
                Storyboard.TargetName="translatePulse1"
                Storyboard.TargetProperty="Y"
                From="0"
                To="60" 
                RepeatBehavior="Forever"
                AutoReverse="False"
                Duration="0:0:2">
            </DoubleAnimation>
            <DoubleAnimation
                Storyboard.TargetName="translatePulse2"
                Storyboard.TargetProperty="Y"
                From="0"
                To="60" 
                RepeatBehavior="Forever"
                AutoReverse="False"
                Duration="0:0:2">
            </DoubleAnimation>
            <DoubleAnimation
